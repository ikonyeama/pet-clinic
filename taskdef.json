{
    "family": "",
    "containerDefinitions": [
        {
            "image": "%REPOSITORY_URI%:v_%BUILD_NUMBER%",
            "name": "PetsContainer",
            "cpu": 500,
            "memory": 512,
            "essential": true,
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80
                }
            ],
            "dockerLabels": {
                "string": "string"
            },
            "logConfiguration": {
                "logDriver": '"wslogs",
                "options": {
                    "awslogs-group": "ECSLogGroup-" + project_name,
                    "awslogs-region": REGION,
                    "awslogs-stream-prefix": project_name
                    }
                }				
        }
    ]
}
